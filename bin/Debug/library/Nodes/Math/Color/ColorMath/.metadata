name "Math"
color 0xFA7A39FF
category "Core/Math/Color"

input colortex A "A"
input colortex B "B"
param enum op "Operation" "Add" "Add","Subtract","Multiply","Divide"
param bool shouldClamp "Clamp?"
param color min "Min"
param color max "Max"
param bool useAlpha "Use Alpha?" true
output colortex outTex "Out"

show min (shouldClamp == true)
show max (shouldClamp == true)

display outTex